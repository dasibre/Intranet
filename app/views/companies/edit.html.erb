<p>Edit Company Details</p>
<%= form_for @company do |f| %>
	<%= f.label :name,"Business Name" %>
	<%= f.text_field :name %>
	<%= f.label :Phone,"Telephone" %>
	<%= f.text_field :phone %>
	<%= f.label :Fax %>
	<%= f.text_field :fax %>
	<%= f.label :website, "Company Url" %>
	<%= f.text_field :website %>
	<%= render :partial => 'addresses/address_form' %><br />
	<%= f.submit 'Update', class: "btn btn-large btn-primary" %>
	<% end %>